@model List<MyAspNetMvcApp.Areas.App.Models.AppControllerAction>

@if (Model.Count > 0)
{
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span class="hidden-sm hidden-md">Examples </span><span class="fa fa-stack-overflow hidden visible-sm visible-md"></span><span class="caret hidden-sm hidden-md"></span></a>
    <ul class="dropdown-menu">
        @foreach (var menu in Model)
        {
            <li><a href="/Examples/@menu.Controller/@menu.Action">@menu.Controller</a></li>
        }
    </ul>
</li>
}
