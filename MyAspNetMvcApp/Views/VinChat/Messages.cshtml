@using MyAspNetMvcApp.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Messages</title>
</head>
<body>
    <div> 
        @foreach(var msg in ViewBag.Messages as List<ChatMessage>)
        {
            <p><strong> @msg.Sender</strong> @msg.Message <em>@msg.SendTime</em></p>

        }
    </div>
</body>
</html>
